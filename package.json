{
  "name": "next-typescript-material-ui-starter",
  "version": "0.1.0",
  "description": "Unofficial Next.js + Typescript + Material UI starter with a latest package",
  "author": "Danang Eko Alfianto <github.com/danangekal>",
  "homepage": "https://github.com/danangekal/next-typescript-material-ui-starter#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danangekal/next-typescript-material-ui-starter.git"
  },
  "bugs": {
    "url": "https://github.com/danangekal/next-typescript-material-ui-starter/issues"
  },
  "license": "MIT",
  "keywords": [
    "nextjs",
    "reactjs",
    "typescript",
    "material-ui",
    "docker",
    "pwa",
    "eslint",
    "prettier",
    "husky",
    "lint-staged"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "telemetry": "next telemetry",
    "lint": "next lint",
    "format": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|css|scss)\" --write",
    "prepare": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@mui/material": "^5.10.8",
    "next": "^12.3.1",
    "next-pwa": "^5.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.8.4",
    "@types/react": "^18.0.21",
    "@types/react-dom": "18.0.6",
    "eslint": "8.25.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "pinst": "^3.0.0",
    "prettier": "^2.7.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=14.19.2"
  }
}
